{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "@eslint-disable-next-line no-unused-vars" : "off",
        "no-empty-function":"error",
        "react-hooks/rules-of-hooks":"off",
        "react-hooks/exhaustive-deps": "error"

    }
}
